{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "ts-node src/server.ts",
    "serve": "yarn build && node ./build/server.js",
    "restart": "rimraf ./build && yarn serve",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./build --onSuccess 'node ./build/server.js'",
    "test": "yarn test-route && yarn test-model",
    "clean-test": "db-migrate --env test reset",
    "up-test": "db-migrate --env test up",
    "run-test": "NODE_ENV=test jasmine-ts --config=jasmine.json",
    "test-route": "yarn clean-test && yarn up-test && yarn run-test --filter='route'",
    "test-model": "yarn clean-test && yarn up-test && yarn run-test --filter='model'",
    "build": "tsc --esModuleInterop src/server.ts --outDir ./build",
    "prepare": "yarn prettier && yarn lint",
    "prettier": "prettier --config .prettierrc.json --write src/**/*.ts",
    "lint": "eslint . --ext .ts"
  },
  "author": "Dieter Jackson",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.12",
    "@types/jasmine": "3.6.9",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/morgan": "^1.9.2",
    "@types/pg": "^8.6.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jasmine": "^4.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.3",
    "prettier": "^2.3.1",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^3.9.10"
  }
}
